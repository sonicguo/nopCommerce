@model IList<PollModel>
@using Nop.Web.Models.Polls;
@if (Model.Count > 0)
{
    <div class="home-page-polls">
        <div class="title">
            <h3>@T("Polls.Title")</h3>
        </div>
        <div>
            @foreach (var poll in Model)
            {
                @Html.Partial("_Poll", poll)
            }
        </div>
       
    </div>
}
